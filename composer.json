{
  "name": "randomstate/camelot-php",
  "description": "PHP Wrapper library for interfacing with the Camelot PDF table extraction library built in Python",
  "type": "library",
  "authors": [
    {
      "name": "Connor Forsyth",
      "email": "connor@randomstate.co.uk"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/process": "^5.0|^6.0",
    "spatie/temporary-directory": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "RandomState\\Camelot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RandomState\\Camelot\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "league/csv": "^9.8"
  },
  "license": "MIT"
}
